{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "conan-install Debug",
            "type": "shell",
            "command": "conan install . --build=missing --output-folder=build/Debug -s build_type=Debug --profile:build=clangDebug",
            "group": "build"
        },
        {
            "label": "conan-install Release",
            "type": "shell",
            "command": "conan install . --build=missing --output-folder=build/Release -s build_type=Release --profile:build=clangRelease",
            "group": "build"
        }
    ]
}